#include <stdio.h>
int globalVar = 20;
void display() {
    int localVar = 10;
    printf("Inside display():\n");
    printf("Local variable = %d\n", localVar);
    printf("Global variable = %d\n", globalVar);
}
int main() {
    display();
    printf("\nInside main():\n");
    printf("Global variable = %d\n", globalVar);
    return 0;
}
