#include <stdio.h>
#include <math.h>  
int main() {
    double a, b, result;
    printf("Enter the base (a): ");
    scanf("%lf", &a);

    printf("Enter the exponent (b): ");
    scanf("%lf", &b);

    result = pow(a, b);  
    printf("%.2lf raised to the power %.2lf = %.2lf", a, b, result);
    return 0;
}
